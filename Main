local folder = workspace.TrainingStations
local plr = game:GetService("Players").LocalPlayer
local bar = folder:GetChildren()[3]
getgenv().toggle = true
local VIM = game:GetService("VirtualInputManager")
loadstring(game:HttpGet("https://raw.githubusercontent.com/batusz/main/roblox/__Anti__Afk__Script__", true))()
spawn(function() -- auto pullup
    while toggle do
        if plr.Stamina.Value <=50 then 
            repeat wait() until plr.Stamina.Value>=1000
        else
            fireclickdetector(bar.ClickDetector,5)
            VIM:SendMouseButtonEvent(900, 400, 0, true, game, 1)
            VIM:SendMouseButtonEvent(900, 400, 0, false, game, 1)
        end
        wait()
    end
end)
spawn(function() -- auto eat
    while toggle do
        repeat
            for i,v in pairs(plr.Backpack:GetChildren()) do
                if v:IsA("Tool") and v.Name == "Chicken" then 
                    local char = plr.Character
                    char.Humanoid:EquipTool(v)
                    char:FindFirstChild(v.Name):Activate()
                    break;
                end
            end
        until 
        wait(32)
    end
end)
